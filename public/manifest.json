{
    "manifest_version": 3,
    "name": "Dog Images Extension",
    "version": "1.0",
    "description": "A Chrome extension that shows random dog images.",
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "favicon.ico",
            "48": "favicon.ico",
            "128": "favicon.ico"
        }
    },
    "permissions": ["activeTab"]
}
